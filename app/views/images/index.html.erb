<p id="notice"><%= notice %></p>

<h1>Images</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @images.each do |image| %>
      <tr>
        <p><%= image.title %> </p>
        <%= link_to 'Show', images_display_url(id: image.id) %>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Upload image', images_upload_path %>
